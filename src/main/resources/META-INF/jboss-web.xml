<security-role name="ADMIN">
    <role-name>ADMIN</role-name>
</security-role>

<security-role name="USER">
    <role-name>USER</role-name>
</security-role>

<security-constraint>
    <web-resource-collection>
        <web-resource>
            <url-pattern>/protected/*</url-pattern>
        </web-resource>
    </web-resource-collection>
    <auth-method>JWT</auth-method>
    <roles>
        <role-name>ADMIN</role-name>
    </roles>
</security-constraint>
